import acm.program.*;

public class PythagoreanTheorem extends ConsoleProgram {
	//Set width and height of text boxes
	private static final int WIDTH = 75;
	private static final int HEIGHT = 30;
	public void run() {
		//Get center x and y coordinates of screen
		int cx = getWidth()/2;
		int cy = getHeight()/2;
		//Set dimensions of boxes
		GRect boxProgram = new GRect(cx,,WIDTH,HEIGHT);
		GRect boxGraphics = new GRect(cx,,WIDTH,HEIGHT);
		GRect boxConsole = new GRect(,,WIDTH,HEIGHT);
		GRect boxDialog = new GRect(,,WIDTH,HEIGHT);
	}
}
