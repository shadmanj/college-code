//Import statements
import javax.swing.*;
import java.util.*;

//Class
class simpleLogin {
  
  public static void main(String[] args){
    /*Obtain username*/
    String username, password;
    username = JOptionPane.showInputDialog(null, "Username: ");
    password = JOptionPane.showInputDialog(null, "Password: ");
    System.out.println(username + ' ' + password);
  }
}